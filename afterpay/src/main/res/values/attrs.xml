<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Afterpay">
        <attr name="afterpayColorScheme" format="enum">
            <enum name="blackOnMint" value="0" />
            <enum name="mintOnBlack" value="1" />
            <enum name="whiteOnBlack" value="2" />
            <enum name="blackOnWhite" value="3" />
        </attr>
        <attr name="afterpayButtonText" format="enum">
            <enum name="payNow" value="0" />
            <enum name="buyNow" value="1" />
            <enum name="checkout" value="2" />
            <enum name="placeOrder" value="3" />
        </attr>
    </declare-styleable>
</resources>
